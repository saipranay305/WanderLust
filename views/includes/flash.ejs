<%if(success && success.length) {%>
<div classs="alert alert-success alert-dismissible fade slow col-6 offset-3" role="alert">
    <%=success %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<%}%>
<%if(error && error.length) {%>
<div class="alert alert-danger alert-dismissible fade slow col-6 offset-3" role="alert">
    <%=error %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<%}%>